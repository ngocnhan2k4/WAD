generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id      Int      @id @default(autoincrement())
  username   String?   @unique
  password   String?
  type String?
}

model product {
  id      Int      @id @default(autoincrement())
  image String?
  name String @unique
  original_price Decimal
  discount_price Decimal
  brand String?
  category String
  description String?
}